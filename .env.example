# Django settings
DJANGO_SETTINGS_MODULE=myproject.settings_prod
SECRET_KEY=your-secret-key-here
DEBUG=False
ALLOWED_HOSTS=your-oracle-domain.com,localhost

# Database settings
DATABASE_URL=postgres://username:password@hostname:5432/database
# Or individual settings
DB_NAME=itensor
DB_USER=postgres
DB_PASSWORD=your-password
DB_HOST=localhost
DB_PORT=5432

# CORS and security
FRONTEND_URL=https://itensor.online
CORS_ALLOW_ALL=False

# Gunicorn settings
GUNICORN_WORKERS=3
GUNICORN_TIMEOUT=120

# Oracle Cloud specific settings
OCI_REGION=us-ashburn-1
OCI_LOGS_OCID=ocid1.log.oc1.iad.your-log-ocid
OCI_NAMESPACE=your-namespace 